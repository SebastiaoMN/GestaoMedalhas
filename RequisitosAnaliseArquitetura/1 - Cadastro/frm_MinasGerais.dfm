  object Q_RelAgraciados: TQuery
    DatabaseName = 'SCM'
    SQL.Strings = (
      'SELECT Dbatj_agraciados.NOME, Dbatj_municipio.NOME'
      
        'FROM DBATJ.MUNICIPIO Dbatj_municipio, DBATJ.AGRACIADOS Dbatj_agr' +
        'aciados'
      'WHERE  (Dbatj_municipio.CODIGO = Dbatj_agraciados.COMR_CODIGO)  '
      
        '   AND  ( (Dbatj_municipio.COMR_CODIGO = Dbatj_municipio.CODIGO)' +
        '  '
      '   AND  (Dbatj_agraciados.ANO = :P_Ano) ) '
      '   and Dbatj_agraciados.data_exclusao is null'
      'and Dbatj_agraciados.tipo_medalha = 1'
      'order by '
      'Dbatj_municipio.NOME')
    Left = 408
    Top = 7
    ParamData = <
      item
        DataType = ftFloat
        Name = 'p_ano'
        ParamType = ptInput
        Value = 0
      end>

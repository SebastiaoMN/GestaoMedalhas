{
  "realm": "gmweb",
  "enabled": true,
  "clients": [
    {
      "clientId": "gmweb-api",
      "secret": "local-secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.signed.response.alg": "RS256"
      }
    },
    {
      "clientId": "gmweb-spa",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost:4200"
      ],
      "protocol": "openid-connect"
    }
  ],
  "roles": {
    "realm": [
      { "name": "agraciados-reader" },
      { "name": "agraciados-writer" },
      { "name": "cadastros-reader" },
      { "name": "cadastros-writer" }
    ],
    "client": {
      "gmweb-api": [
        { "name": "agraciados:read" },
        { "name": "agraciados:write" },
        { "name": "cadastros:read" },
        { "name": "cadastros:write" }
      ]
    }
  }
}
